{
  "name": "ps2svg",
  "version": "2.0.2",
  "description": "Convert PostScript to SVG",
  "main": "./dist/ps2svg.js",
  "type": "module",
  "author": "HÃ©rcules Ramlow <https://www.linkedin.com/in/hercules-ramlow> (https://github.com/HRsniper)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HRsniper/ps2svg.git"
  },
  "homepage": "https://github.com/HRsniper/ps2svg#readme",
  "bugs": {
    "url": "https://github.com/HRsniper/ps2svg/issues"
  },
  "scripts": {
    "build": "npx tsc",
    "color2rgb": "npm run build && node ./dist/color2rgb.js",
    "ps2svg": "npm run build && node ./dist/ps2svg.js",
    "prestart": "npm run build && npm run prettier && npm run eslint",
    "start": "node ./dist/ps2svg.js",
    "prettier": "npx prettier --write .",
    "eslint": "npx eslint ./src --fix",
    "prepublishOnly": "npm run prestart",
    "v1": "npm run build && node ./dist/v1/ps2svg_v1.js",
    "v2": "npm run build && node ./dist/v2/cli.js",
    "t": "npm run build && node ./dist/ps2svg.js ps_files/example_full v2 && node ./dist/v2/ps2svg_work.js ps_files/example_full work"
  },
  "engines": {
    "node": ">=24.8.0"
  },
  "bin": {
    "ps2svg": "./bin/ps2svg.js"
  },
  "keywords": [
    "ps",
    "svg",
    "convert",
    "converter",
    "pstosvg",
    "ps2svg"
  ],
  "devDependencies": {
    "globals": "^16.4.0",
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2",
    "@eslint/js": "^9.36.0",
    "typescript-eslint": "^8.44.1",
    "prettier": "^3.6.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4"
  },
  "types": "./dist/*.d.ts"
